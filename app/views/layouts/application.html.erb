<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= csrf_meta_tag %>
  <%= render 'layouts/stylesheets' %>
</head>
<body>
	<div class="comtainer">
		<%= render 'layouts/header' %>
		<section class="round">
    		<% flash.each do |name, msg| %>
      			<%= content_tag :div, msg, :id => "flash_#{name}" %>
    		<% end %>

			<%= yield %>
		</section>
	<%= render 'layouts/footer' %>
	<%= debug(params) if Rails.env.development? %>
</body>
</html>
